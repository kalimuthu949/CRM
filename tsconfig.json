{
  "extends": "./node_modules/@microsoft/rush-stack-compiler-4.7/includes/tsconfig-web.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "inlineSources": false,
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@microsoft",
      ".src/typings"
    ],
    "types": ["webpack-env"],
    "lib": [
      "es5",
      "dom",
      "ES6",
      "es2015.collection",
      "es2015.promise",
      "ES2016",
      "ES2017.Object",
      "ES2019",
      "ES2021"
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "custom-declarations.d.ts"]
}
